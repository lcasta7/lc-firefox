{
  "manifest_version": 2,
  "name": "lc-shortcuts",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "commands": {
    "next-tab": {
      "suggested_key": {
        "default": "Ctrl+9"
      },
      "description": "Go to next tab"
    },
    "prev-tab": {
      "suggested_key": {
        "default": "Ctrl+8"
      },
      "description": "Go to previous tab"
    },
    "prior-tab": {
      "suggested_key": {
        "default": "Ctrl+0"
      },
      "description": "Go to previous tab"
    }
  }
}
